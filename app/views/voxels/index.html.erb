<h1>Listing voxels</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Title</th>
      <th>Data json</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @voxels.each do |voxel| %>
      <tr>
        <td><%= voxel.user_id %></td>
        <td><%= voxel.title %></td>
        <td><%= voxel.data_json %></td>
        <td><%= link_to 'Show', voxel %></td>
        <td><%= link_to 'Edit', edit_voxel_path(voxel) %></td>
        <td><%= link_to 'Destroy', voxel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voxel', new_voxel_path %>
